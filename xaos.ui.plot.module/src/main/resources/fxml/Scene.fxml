<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane id="AnchorPane" prefHeight="625.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Demo.FXMLController">
    <children>
        <Button fx:id="button" layoutX="29.0" layoutY="29.0" onAction="#handleErrorButton" text="Error Bars" />
      <BorderPane fx:id="borderpane" layoutX="59.0" layoutY="76.0" prefHeight="473.0" prefWidth="541.0" />
      <Button fx:id="yaxisbutton" layoutX="-21.0" layoutY="299.0" mnemonicParsing="false" onAction="#handleYLogButton" rotate="-90.0" text="log/norm axis" />
      <Button fx:id="xaxisbutton" layoutX="279.0" layoutY="563.0" mnemonicParsing="false" onAction="#handleXLogButton" text="log/norm axis"  />
      <ComboBox fx:id="chartchoice" layoutX="175.0" layoutY="29.0" prefHeight="27.0" prefWidth="359.0" promptText="Examples" />
      <Button layoutX="30.0" layoutY="563.0" mnemonicParsing="false" onAction="#handleResetButton" text="Reset Axes" />
    </children>
</AnchorPane>
